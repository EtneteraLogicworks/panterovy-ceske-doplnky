#!/bin/bash

readonly PLIST_BUDDY='/usr/libexec/PlistBuddy'
readonly PREFERENCES_FILE="Library/Preferences/.GlobalPreferences.plist"


if [ "$3" != '/' ]; then
    # Installing to non-root volume
    exit 0
fi

readonly CONSOLE_ID=$(echo "show State:/Users/ConsoleUser" | /usr/sbin/scutil | /usr/bin/awk '/UID/ { print $3 }')
readonly CONSOLE_USER=$(stat -f "%Su" "/dev/console")
readonly CONSOLE_USER_HOME=$(dscl . read "/Users/${CONSOLE_USER}" NFSHomeDirectory | /usr/bin/awk '{ print $2 }')

if [ -z "$CONSOLE_USER" -o "$(/usr/bin/id -u $CONSOLE_USER)" != "$CONSOLE_ID" ]; then
    # Nobody is logged in
    exit 0
fi

"$PLIST_BUDDY" -c "Delete NSPreferredSpellServerLanguage" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServerLanguage string" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Set NSPreferredSpellServerLanguage cs_CZ" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"

"$PLIST_BUDDY" -c "Delete NSPreferredSpellServers" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:0 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:0:0 string 'cs_CZ'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:0:1 string 'Open'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:1 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:1:0 string 'en'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:1:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:2 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:2:0 string 'da'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:2:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:3 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:3:0 string 'de'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:3:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:4 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:4:0 string 'es'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:4:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:5 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:5:0 string 'fr'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:5:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:6 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:6:0 string 'it'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:6:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:7 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:7:0 string 'nl'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:7:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:8 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:8:0 string 'nb'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:8:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:9 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:9:0 string 'pl_PL'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:9:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:10 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:10:0 string 'pt_BR'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:10:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:11 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:11:0 string 'sv'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:11:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:12 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:12:0 string 'tr'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:12:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:13 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:13:0 string 'ru'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSPreferredSpellServers:13:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"

"$PLIST_BUDDY" -c "Delete NSSpellServers" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
