"$PLIST_BUDDY" -c "Add NSSpellServers:3:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:4 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:4:0 string 'en_CA'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:4:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:5 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:5:0 string 'en_IN'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:5:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:6 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
