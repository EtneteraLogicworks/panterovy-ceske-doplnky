"$PLIST_BUDDY" -c "Add NSSpellServers:13 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:13:0 string 'pl_PL'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:13:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:14 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:14:0 string 'pt_BR'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:14:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:15 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:15:0 string 'pt_PT'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:15:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:16 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:16:0 string 'sv'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:16:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:17 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:17:0 string 'tr'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:17:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:18 array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:18:0 string 'ru'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add NSSpellServers:18:1 string 'Apple'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"

"$PLIST_BUDDY" -c "Delete com.apple.DictionaryServices" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add com.apple.DictionaryServices dict" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add com.apple.DictionaryServices:DCSActiveDictionaries array" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add com.apple.DictionaryServices:DCSActiveDictionaries:0 string '/Library/Dictionaries/Anglicko-český slovník.dictionary'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add com.apple.DictionaryServices:DCSActiveDictionaries:1 string '/Library/Dictionaries/Slovník Hantecu.dictionary'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add com.apple.DictionaryServices:DCSActiveDictionaries:2 string '/System/Library/Frameworks/CoreServices.framework/Frameworks/DictionaryServices.framework/Resources/Wikipedia.wikipediadictionary'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add com.apple.DictionaryServices:DCSActiveDictionaries:3 string '/Library/Dictionaries/Apple Dictionary.dictionary'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add com.apple.DictionaryServices:DCSDictionaryPrefs dict" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add com.apple.DictionaryServices:DCSDictionaryPrefs:com.apple.dictionary.Wikipedia dict" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
"$PLIST_BUDDY" -c "Add com.apple.DictionaryServices:DCSDictionaryPrefs:com.apple.dictionary.Wikipedia:language-it string '0'" "${CONSOLE_USER_HOME}/${PREFERENCES_FILE}"
